{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "Another opinionated boilerplate for building web apps",
  "main": "src/index.js",
  "repository": "https://github.com/albertfdp/boilerplate",
  "author": "Albert Fern√°ndez <albert@nandez.cat>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "NODE_ENV=production webpack --progress",
    "format": "prettier --single-quote --write {config,src,test}/**/*.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "travis": "yarn run format && yarn run test"
  },
  "devDependencies": {
    "babel-core": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-loader": "6.4.1",
    "babel-plugin-module-resolver": "2.5.0",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-preset-env": "1.2.2",
    "babel-preset-react": "6.23.0",
    "css-loader": "0.27.3",
    "csscomb": "4.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.10.1",
    "html-webpack-plugin": "2.28.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "19.0.2",
    "offline-plugin": "4.6.2",
    "postcss-cssnext": "2.10.0",
    "postcss-import": "9.1.0",
    "postcss-loader": "1.3.3",
    "prettier": "0.22.0",
    "react": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.4.2",
    "react-svg-loader": "1.1.1",
    "react-test-renderer": "15.4.2",
    "style-loader": "0.16.0",
    "unexpected": "10.26.3",
    "unexpected-react": "3.5.2",
    "url-loader": "0.5.8",
    "webpack": "2.3.1",
    "webpack-dev-server": "2.4.2"
  },
  "standard": {
    "parser": "babel-eslint",
    "global": [
      "__DEV__",
      "__PRODUCTION__",
      "describe",
      "it"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/test/stubs/styles.js"
    },
    "testRegex": "\\.*(spec\\.js)$"
  }
}
